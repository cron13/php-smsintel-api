{
    "name": "seregazhuk/smsintel-api",
    "description": "A PHP wrapper for the SmsIntel api. Provides one interface for both XML and JSON API requests.",
    "keywords": [
        "php",
        "lib",
        "smsintel"
    ],
    "type": "package",
    "minimum-stability": "dev",
    "license": "MIT",
    "authors": [
        {
            "name": "Zhuk Sergey",
            "email": "seregazhuk88@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "ext-curl": "*",
        "guzzlehttp/guzzle": "3"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.0",
        "codeclimate/php-test-reporter": "^0.3.2",
        "mockery/mockery": "^0.9.5"
    },
    "autoload": {
        "psr-4": {
            "seregazhuk\\SmsIntel\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "seregazhuk\\SmsIntel\\": "src",
            "seregazhuk\\tests\\": "tests"
        }
    }
}
